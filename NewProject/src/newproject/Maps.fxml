<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #001219;" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="newproject.MapsController">
    <stylesheets>
        <URL value="@maps.css" />
    </stylesheets>
   <children>
      <WebView fx:id="webView" layoutY="100.0" prefHeight="500.0" />
      <TextField fx:id="searchTextField" layoutY="26.0" prefHeight="27.0" prefWidth="800.0" style="-fx-background-color: white;" />
      <Button fx:id="load" layoutX="14.0" layoutY="59.0" mnemonicParsing="false" onAction="#loadPage" text="Load" />
      <Button layoutX="89.0" layoutY="59.0" mnemonicParsing="false" onAction="#zoomIn" text="Zoom in" />
      <Button layoutX="153.0" layoutY="59.0" mnemonicParsing="false" onAction="#zoomOut" text="Zoom out" />
   </children>
</AnchorPane>
